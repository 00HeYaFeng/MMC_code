<view><block wx:if="{{tabType}}"><view><image class="banner_left" src="../../static/images/refresh.png" data-event-opts="{{[['tap',[['refreshs',['$event']]]]]}}" bindtap="__e"></image><image class="banner_right" src="../../static/images/open.png" data-event-opts="{{[['tap',[['test',['$event']]]]]}}" bindtap="__e"></image><block wx:for="{{iwy}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><view class="main_body"><view class="body_content"><view class="body_mp4" style="background-image:url();" id="{{item.id}}" data-event-opts="{{[['tap',[['videoClick',['$event']]]]]}}" bindtap="__e"><image style="width:100%;height:100%;" src="{{item.cover}}"></image><image class="body_pause" src="../../static/images/pause.png"></image></view><view class="body_userinfo" style="margin-top:3%;"><view class="body_user"><image class="user_head" src="{{item.avatar}}"></image><view class="user_name">{{item.name}}</view></view><view class="body_button"><image class="heart_icon" id="{{'mediaLike_'+index}}" src="{{item.good}}" data-event-opts="{{[['tap',[['like',['$event']]]]]}}" bindtap="__e"></image><view class="body_font">{{item.like}}</view></view><view class="body_button" id="{{item.id}}" data-event-opts="{{[['tap',[['videoClick',['$event']]]]]}}" bindtap="__e"><image class="comment_icon" src="../../static/images/comment.png"></image><view class="body_font">{{item.comments}}</view></view><button class="indexButton" data-description="{{item.description}}" data-id="{{item.id}}" data-img="{{item.cover}}" data-title="{{item.title}}" id="index" data-event-opts="{{[['tap',[['wxshare',['$event']]]]]}}" bindtap="__e"><image class="body_share" src="../../static/images/share.png"></image></button></view></view></view></view></block></view></block></view>