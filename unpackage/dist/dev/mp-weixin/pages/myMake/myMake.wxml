<view><block wx:for="{{video}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="main_body"><view class="body_content"><text class="myMakeText">{{item.title}}</text><view class="body_mp4" style="{{'background-image:'+('url('+item.mediaCover+')')+';'}}" id="{{index}}" data-event-opts="{{[['tap',[['videoClick',['$event']]]]]}}" bindtap="__e"><image class="body_pause" src="../../static/images/pause.png"></image></view><view class="body_userinfo"><view class="body_user"><image class="user_head" src="{{item.head?item.head:defaultHead}}"></image><view class="user_name">{{item.name}}</view></view><view class="user_mediaType">{{''+(item.mediaType=='person'?'个人视频':'企业视频')+''}}</view><button class="indexButton" open-type="share" id="index"><image class="body_share" src="../../static/images/share.png"></image></button></view></view></view></block></block></view>