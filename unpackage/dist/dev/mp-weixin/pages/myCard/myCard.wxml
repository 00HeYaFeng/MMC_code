<view><view class="CardTop"><view data-event-opts="{{[['tap',[['personClick',['$event']]]]]}}" class="CardPerson" style="{{(personStyle)}}" bindtap="__e">个人</view><view data-event-opts="{{[['tap',[['companyClick',['$event']]]]]}}" class="CardCompany" style="{{(companyStyle)}}" bindtap="__e">企业</view></view><block wx:if="{{hiddenSwitch}}"><block><view class="personMedia"><image hidden="{{!(!personUpdate&&mediaData.mediaAddress!='')}}" src="../../static/images/error.png" mode data-event-opts="{{[['tap',[['updatevideo',['person']]]]]}}" bindtap="__e"></image><text class="personMediaText">个人视频展示</text><block wx:if="{{mediaData.mediaAddress==''}}"><block><view data-event-opts="{{[['tap',[['makeVideo',['$event']]]]]}}" class="mediaView" bindtap="__e"><image class="mediaImage" src="{{mediaFilmImage}}" mode></image><text class="mediaViewText">拍摄个人视频简介</text></view></block></block><block wx:else><block><video class="personMediaVideo" src="{{mediaData.mediaAddress}}" controls autoplay page-gesture="true" show-fullscreen-btn show-play-btn show-center-play-btn enable-progress-gesture vslide-gesture-in-fullscreen></video></block></block></view><view class="personData"><text class="personDataText">个人资料</text><block wx:if="{{personUpdate}}"><block><text data-event-opts="{{[['tap',[['personUpdateBool',['$event']]]]]}}" class="personDataSave" bindtap="__e">修改资料</text></block></block><block wx:else><block><text data-event-opts="{{[['tap',[['personDataSave',['$event']]]]]}}" class="personDataSave" bindtap="__e">保存资料</text></block></block><view class="personDataView"><view class="personDataName"><text class="personDataNameText">姓       名:</text><textarea class="personDataTextarea" disabled="{{personUpdate}}" type="text" placeholder="请输入姓名" data-event-opts="{{[['input',[['personDataNameChange',['$event']]]]]}}" value="{{personData.personName}}" bindinput="__e"></textarea></view><view class="personDataName"><text class="personDataNameText">电       话:</text><textarea class="personDataTextarea" disabled="{{personUpdate}}" type="text" placeholder="请输入电话" data-event-opts="{{[['input',[['personDataCallChange',['$event']]]]]}}" value="{{personData.personCall}}" bindinput="__e"></textarea></view><view class="personDataName"><text class="personDataNameText">邮       箱:</text><textarea class="personDataTextarea" disabled="{{personUpdate}}" type="text" placeholder="请输入邮箱" data-event-opts="{{[['input',[['personDataEmailChange',['$event']]]]]}}" value="{{personData.personEmail}}" bindinput="__e"></textarea></view><view class="personDataAddress"><text class="personDataNameText">地       址:</text><textarea class="personDataTextareaExperience" disabled="{{personUpdate}}" type="text" placeholder="请输入详细地址...." data-event-opts="{{[['input',[['personDataAddressChange',['$event']]]]]}}" value="{{personData.personAddress}}" bindinput="__e"></textarea></view><view class="personDataExperience"><text class="personDataNameText">个人简历</text><textarea class="personDataTextareaExperience" disabled="{{personUpdate}}" type="text" placeholder="请输入个人经历简介...." data-event-opts="{{[['input',[['personDataExperienceChange',['$event']]]]]}}" value="{{personData.personExperience}}" bindinput="__e"></textarea></view></view></view><view class="personProject"><text class="personProjectTitle">项目资料</text><text data-event-opts="{{[['tap',[['personProjectAdd',['$event']]]]]}}" style="float:left;margin-left:85rpx;" bindtap="__e">长按删除</text><text data-event-opts="{{[['tap',[['personProjectAdd',['$event']]]]]}}" class="personProjectAdd" bindtap="__e">添加项目</text><block wx:if="{{projectData!=''}}"><block><block wx:for="{{projectData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="personProjectView" id="{{item.project_id}}" data-event-opts="{{[['tap',[['switchAddProject',['$event']]]],['longtap',[['longtaped',['$0','$1'],[[['projectData','',index,'project_id']],[['projectData','',index,'project_name']]]]]]]}}" bindtap="__e" bindlongtap="__e"><view class="personProjectHead"><view class="personProjectHeadView"><text class="personProjectName">{{item.project_name}}</text><text class="personProjectTime">{{item.project_first+"——"+(item.project_end!=""?item.project_end:'暂无')}}</text></view><text class="personProjectWork">{{"担任职位:"+item.project_role}}</text></view><view class="personProjectDescribe"><text class="personProjectDescribeText">{{item.project_describe}}</text></view></view></block></block></block></block><block wx:else><block><view class="personProjectView"><view class="personProjectHead"><view class="personProjectHeadView"><text class="personProjectName">暂无项目</text><text class="personProjectTime"></text></view><text class="personProjectWork"></text></view></view></block></block></view></block></block><block wx:else><block><block wx:if="{{companyData.address}}"><block><view style="display:flex;float:left;width:100%;align-content:center;"><text class="companyMediaText">企业视频展示</text><image src="../../static/images/error.png" mode data-event-opts="{{[['tap',[['updatevideo',['company']]]]]}}" bindtap="__e"></image></view><video class="companyMediaVideo" src="{{companyData.address}}" controls autoplay page-gesture="true" show-fullscreen-btn show-play-btn show-center-play-btn enable-progress-gesture vslide-gesture-in-fullscreen></video></block></block><block wx:else><block><view data-event-opts="{{[['tap',[['makeCompanyVideo',['$event']]]]]}}" class="companyMediaView" bindtap="__e"><image class="companyMediaViewImageLeft" src="{{mediaFilmImage}}" mode></image><text class="companyMediaViewText">拍摄企业简介</text><image class="companyMediaViewImageRight" src="{{backRight}}" mode></image></view></block></block><view class="companyNameView"><text class="companyNameViewName">企业名称</text><textarea class="companyNameViewText" disabled="{{companyUpdate}}" placeholder="请输入贵公司的企业名字" data-event-opts="{{[['input',[['companyNameViewText',['$event']]]]]}}" value="{{companyData.companyName}}" bindinput="__e"></textarea></view><view class="companyNameView"><text class="companyNameViewName">业务范围</text><textarea class="companyNameViewText" disabled="{{companyUpdate}}" placeholder="请输入贵公司的业务范围" data-event-opts="{{[['input',[['companyAchievementViewText',['$event']]]]]}}" value="{{companyData.companyRange}}" bindinput="__e"></textarea></view><view class="companyNameView"><text class="companyNameViewName">办公地址</text><textarea class="companyNameViewText" disabled="{{companyUpdate}}" placeholder="请输入贵公司的办公地址" data-event-opts="{{[['input',[['companyAddressViewText',['$event']]]]]}}" value="{{companyData.companyAddress}}" bindinput="__e"></textarea></view><view class="companyNameView"><text class="companyNameViewName">企业联系电话</text><textarea class="companyNameViewText" disabled="{{companyUpdate}}" placeholder="请输入贵公司的联系电话" data-event-opts="{{[['input',[['companyCallViewText',['$event']]]]]}}" value="{{companyData.companyCall}}" bindinput="__e"></textarea></view><view class="companyNameView"><text class="companyNameViewName">企业邮箱</text><textarea class="companyNameViewText" disabled="{{companyUpdate}}" placeholder="请输入贵公司的邮箱账号" data-event-opts="{{[['input',[['companyEmailViewText',['$event']]]]]}}" value="{{companyData.companyEmail}}" bindinput="__e"></textarea></view><view class="companyDevelopView"><text class="companyNameViewName">发展历程</text><textarea class="companyDevelopViewText" disabled="{{companyUpdate}}" placeholder="请输入贵公司的发展历程" maxlength="300" data-event-opts="{{[['input',[['companyDevelopViewText',['$event']]]]]}}" value="{{companyData.companyDevelop}}" bindinput="__e"></textarea></view><block wx:if="{{companyUpdate}}"><block><button data-event-opts="{{[['tap',[['companyUpdateClick',['$event']]]]]}}" class="companyButtonSave" bindtap="__e">修改</button></block></block><block wx:else><block><button data-event-opts="{{[['tap',[['companySaveClick',['$event']]]]]}}" class="companyButtonSave" bindtap="__e">保存</button></block></block><button data-event-opts="{{[['tap',[['companyPreviewClick',['$event']]]]]}}" class="companyButtonPreview" bindtap="__e">生成预览页面</button></block></block></view>